{
  "name": "@oamm/textor",
  "version": "1.0.15",
  "description": "Safe, deterministic scaffolding and refactoring tool for Astro projects",
  "type": "module",
  "bin": {
    "textor": "./dist/bin/textor.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "textor": "node dist/bin/textor.js",
    "test": "vitest run",
    "build": "rollup -c && rimraf dist/src",
    "clean": "rimraf dist",
    "dev": "rollup -c -w",
    "types:bundle": "rollup -c",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "astro",
    "scaffolding",
    "cli",
    "tooling"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.4",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oamm/textor.git"
  },
  "bugs": {
    "url": "https://github.com/oamm/textor/issues"
  },
  "homepage": "https://github.com/oamm/textor#readme"
}
